{
  "name": "web-route",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "child_process": "^1.0.2",
    "clear": "^0.0.1",
    "crawler": "^1.0.5",
    "eslint": "^4.7.2",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "gulp-bump": "^2.7.0",
    "gulp-cached": "^1.1.1",
    "gulp-eslint": "^4.0.0",
    "gulp-help": "^1.6.1",
    "gulp-watchman": "^2.0.2",
    "history": "^4.7.2",
    "jest": "^21.1.0",
    "jest-junit-reporter": "^1.1.0",
    "lodash": "^4.17.4",
    "path": "^0.12.7",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "next",
    "react-scripts": "1.0.13",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-thunk": "^2.2.0",
    "stream": "^0.0.2",
    "unexpected": "^10.35.0",
    "unexpected-set": "^1.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "flow": "flow",
    "flow:stop": "./node_modules/.bin/flow stop",
    "flow:watch": "gulp flow:watch --silent",
    "eslint": "gulp eslint",
    "eslint:watch": "gulp eslint:watch --silent",
    "jest": "jest --no-watchman",
    "jest:coverage": "jest --coverage",
    "jest:coverage:watch": "jest --coverage --watchAll --watchman",
    "jest:watch": "jest --watch --watchman"
  },
  "jest": {
    "automock": false,
    "testResultsProcessor": "./node_modules/jest-junit-reporter",
    "roots": [
      "<rootDir>/src",
      "<rootDir>/../common"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/react-scripts/config/jest/babelTransform.js",
      "^.+\\.css$": "<rootDir>/node_modules/react-scripts/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/node_modules/react-scripts/config/jest/fileTransform.js"
    },
    "setupFiles": [
      "<rootDir>/node_modules/react-scripts/config/polyfills.js"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "html"
    ],
    "collectCoverageFrom": [
      "<rootDir>/src/components/**/*.js",
      "<rootDir>/src/lib/**/*.js"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-flow": "^6.23.0",
    "eslint": "^4.6.1",
    "eslint-plugin-react": "^7.3.0",
    "flow-bin": "^0.55.0",
    "gulp": "^3.9.1"
  }
}
