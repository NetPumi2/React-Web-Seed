{
  "root": true,
  "parser": "babel-eslint",
  "extends": "airbnb",
  "plugins": [
    "flowtype"
  ],
  "env": {
    "jest": true,
    "browser": true,
    "es6": true
  },
  "globals": {
    "uexpect": true,
    "$Keys": true
  },
  "settings": {
    "flowtype": {
      "onlyFilesWithFlowAnnotation": false
    }
  },
  "rules": {
    "flowtype/boolean-style": 2,
    "flowtype/delimiter-dangle": [2, "always-multiline"],
    "flowtype/generic-spacing": 2,
    "flowtype/require-valid-file-annotation": 0,
    "flowtype/semi": 2,
    "flowtype/space-after-type-colon": 2,
    "flowtype/space-before-generic-bracket": 2,
    "flowtype/space-before-type-colon": 2,
    "flowtype/type-id-match": [2, "^([A-Z][a-z0-9]*)+$"],
    "flowtype/union-intersection-spacing": 2,
    "flowtype/no-primitive-constructor-types": 0,
    "flowtype/object-type-delimiter": 0,
    "global-require": 0,
    "import/no-duplicates": 2,
    "import/no-named-as-default": 0,
    "import/prefer-default-export": 0,
    "import/first": 1,
    "no-duplicate-imports": 0,
    "no-shadow": 0,
    "no-underscore-dangle": ["error", { "allow": ["__"] }],
    "no-mutable-exports": "off",
    "spaced-comment": 0,
    "react/jsx-filename-extension": 0,
    "react/no-unused-prop-types": 0,
    "react/prefer-stateless-function": 0,
    "react/require-default-props": 0,
    "jsx-a11y/img-has-alt": 0,
    "jsx-a11y/alt-text": 0,
    "jsx-a11y/interactive-supports-focus": 0,
    "jsx-a11y/media-has-caption": 0,
    "jsx-a11y/no-interactive-element-to-noninteractive-role": 0,
    "jsx-a11y/no-noninteractive-element-interactions": 0,
    "jsx-a11y/no-noninteractive-element-to-interactive-role": 0,
    "jsx-a11y/no-noninteractive-tabindex": 0,
    "react/sort-comp": [2, {
      "order": [
        "type-annotations",
        "static-methods",
        "lifecycle",
        "everything-else",
        "render"
      ]
    }]
  }
}
